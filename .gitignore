# ==========================================
# Laravel + Node.js Project .gitignore
# ==========================================

# ========== LARAVEL ==========
# Laravel specific
/laravel/vendor/
/laravel/node_modules/
/laravel/public/hot
/laravel/public/storage
/laravel/storage/*.key
/laravel/storage/logs/*.log
/laravel/storage/framework/cache/*
/laravel/storage/framework/sessions/*
/laravel/storage/framework/testing/*
/laravel/storage/framework/views/*
/laravel/bootstrap/cache/*

# Environment files (permitiendo .env para el proyecto)
# /laravel/.env  # Comentado para permitir subir .env
/laravel/.env.backup
/laravel/.env.production

# IDE and editor files
.vscode/
.idea/
*.swp
*.swo
*~

# ========== NODE.JS ==========
# Dependencies
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# Runtime data
pids
*.pid
*.seed
*.pid.lock

# Coverage directory used by tools like istanbul
coverage/
*.lcov

# nyc test coverage
.nyc_output

# node-waf configuration
.lock-wscript

# Compiled binary addons
build/Release

# Dependency directories
jspm_packages/

# Snowpack dependency directory
web_modules/

# Optional npm cache directory
.npm

# Optional eslint cache
.eslintcache

# Optional stylelint cache
.stylelintcache

# Microbundle cache
.rpt2_cache/
.rts2_cache_cjs/
.rts2_cache_es/
.rts2_cache_umd/

# Optional REPL history
.node_repl_history

# Output of 'npm pack'
*.tgz

# Yarn Integrity file
.yarn-integrity

# dotenv environment variable files (permitiendo .env para el proyecto)
